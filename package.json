{
  "name": "soundcloud-api-v6",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build && node ./build.custom.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "events": "^3.0.0",
    "firebase": "^5.5.0",
    "fontfaceobserver": "^2.0.13",
    "mitt": "^1.1.3",
    "no-outline-on-click": "0.0.2",
    "querystring": "^0.2.0",
    "register-service-worker": "^1.0.0",
    "simplebar": "^3.0.0-beta.0",
    "soundcloud-audio": "^1.3.1",
    "typescript": "^3.0.1",
    "vue": "^2.5.16",
    "vue-analytics": "^5.16.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "0.0.6",
    "@vue/cli-plugin-babel": "^3.0.0-beta.16",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.16",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.16",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.16",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.16",
    "@vue/cli-service": "^3.0.0-beta.16",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.16",
    "@vue/eslint-config-typescript": "^3.0.0-beta.16",
    "babel-plugin-groundskeeper-willie": "^1.3.2",
    "lint-staged": "^6.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.16",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "no-console": "off",
      "strict": "off",
      "import/prefer-default-export": "off",
      "import/no-unresolved": "off",
      "no-restricted-globals": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "camelcase": "off",
      "no-alert": "off",
      "default-case": "off",
      "global-require": "off",
      "max-len": "off",
      "no-restricted-syntax": "off",
      "no-return-assign": "off",
      "vue/require-prop-types": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
