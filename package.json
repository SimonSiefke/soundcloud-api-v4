{
  "name": "soundcloud-api-v44",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --modern && node ./build.custom/index.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:e2e",
    "test:bundlesize": "bundlesize"
  },
  "dependencies": {
    "@simon-siefke/vue-custom-scrollbar": "0.0.12",
    "@types/fontfaceobserver": "0.0.6",
    "abortcontroller-polyfill": "^1.2.1",
    "axios": "^0.18.0",
    "fontfaceobserver": "^2.1.0",
    "googleapis": "^36.0.0",
    "mitt": "^1.1.3",
    "querystring": "^0.2.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-idle-component": "0.0.1",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "bundlesize": "^0.17.0",
    "eslint-plugin-vue-a11y": "0.0.28",
    "lint-staged": "^7.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": "off",
      "vue/require-prop-types": false,
      "vue-a11y/alt-text": 2,
      "vue-a11y/accessible-emoji": 2,
      "vue-a11y/anchor-has-content": 2,
      "vue-a11y/click-events-have-key-events": 2,
      "vue-a11y/mouse-events-have-key-events": 2,
      "vue-a11y/no-autofocus": 2,
      "vue-a11y/no-onchange": 2,
      "vue-a11y/tabindex-no-positive": 2,
      "vue-a11y/heading-has-content": 2,
      "vue-a11y/form-has-label": 2,
      "vue-a11y/aria-role": 2,
      "vue-a11y/aria-props": 2,
      "vue-a11y/aria-unsupported-elements": 2,
      "vue-a11y/role-has-required-aria-props": 2
    },
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    },
    "plugins": [
      "vue-a11y"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/js/chunk-vendors.*.js",
      "maxSize": "55 kB"
    },
    {
      "path": "./dist/js/app.*.js",
      "maxSize": "6 kB"
    }
  ]
}
